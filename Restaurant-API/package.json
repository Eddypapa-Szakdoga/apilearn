{
  "name": "restaurant-api",
  "version": "0.0.1",
  "description": "Restaurant API",
  "main": "dist",
  "scripts": {
    "dev": "NODE_ENV=development nodemon -w src --exec \"babel-node src --presets es2020, stage-0\"",
    "build": "babel src -s -D -d dist --preset es2020,stage-0",
    "start": "NODE_ENV=production pm2 start dist",
    "prestart": "npm run -s build",
    "lint": "eslint src",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "eslintConfig": {
    "parserOptions": {
      "ecmaVersion": 7,
      "sourceType": "module"
    },
    "env": {
      "node": true
    },
    "rules": {
      "no-console": 0,
      "no-unused-vars": 1
    }
  },
  "author": "Adam Toth",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "core-js": "^3.6.4",
    "express": "^4.17.1",
    "mongoose": "^5.9.6"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.3",
    "babel-eslint": "^10.1.0",
    "babel-preset-es2020": "^1.0.2",
    "babel-preset-stage-0": "^6.24.1",
    "eslint": "^6.8.0",
    "nodemon": "^2.0.2"
  }
}
